{
  "application": "foo-app",
  "name": "foo-pipeline-direct",
  "keepWaitingPipelines": false,
  "stages": [
    {
      "account": "default",
      "cloudProvider": "kubernetes",
      "manifestArtifactAccount": "gcs-sandbox--spinnaker-3",
      "manifests": [
        {
          "apiVersion": "extensions/v1beta1",
          "kind": "Deployment",
          "metadata": {
            "name": "foo-deployment",
            "labels": {
              "app": "foo"
            }
          },
          "spec": {
            "replicas": 2,
            "selector": {
              "matchLabels": {
                "app": "foo"
              }
            },
            "template": {
              "metadata": {
                "labels": {
                  "app": "foo"
                }
              },
              "spec": {
                "containers": [
                  {
                    "name": "nginx",
                    "image": "nginx",
                    "imagePullPolicy": "Always",
                    "ports": [
                      {
                        "containerPort": 80
                      }
                    ]
                  }
                ]
              }
            }
          }
        }
      ],
      "moniker": {
        "app": "foo-app"
      },
      "name": "Deploy (Manifest)",
      "refId": "1",
      "relationships": {
        "loadBalancers": [],
        "securityGroups": []
      },
      "requisiteStageRefIds": [],
      "skipExpressionEvaluation": false,
      "source": "text",
      "trafficManagement": {
        "enabled": false,
        "options": {
          "enableTraffic": false,
          "services": []
        }
      },
      "type": "deployManifest"
    }
  ],
  "triggers": []
}
